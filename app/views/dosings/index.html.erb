<h1>Listing dosings</h1>

<table>
  <thead>
    <tr>
      <th>Attack</th>
      <th>Drug</th>
      <th>Dosed at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @dosings.each do |dosing| %>
      <tr>
        <td><%= dosing.attack_id %></td>
        <td><%= dosing.drug_id %></td>
        <td><%= dosing.dosed_at %></td>
        <td><%= link_to 'Show', dosing %></td>
        <td><%= link_to 'Edit', edit_dosing_path(dosing) %></td>
        <td><%= link_to 'Destroy', dosing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dosing', new_dosing_path %>
